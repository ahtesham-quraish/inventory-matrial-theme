{
  "name": "material-dashboard-react",
  "version": "1.6.0",
  "description": "Material Dashboard React. Coded by Creative Tim",
  "private": false,
  "main": "public/electron.js",
  "dependencies": {
    "@material-ui/core": "3.9.2",
    "@material-ui/icons": "3.0.2",
    "axios": "^0.18.0",
    "chartist": "0.10.1",
    "classnames": "2.2.6",
    "cross-env": "5.1.6",
    "electron": "^5.0.7",
    "electron-builder": "^21.1.1",
    "electron-is-dev": "^1.1.0",
    "fs": "^0.0.1-security",
    "history": "4.7.2",
    "os": "^0.1.1",
    "perfect-scrollbar": "1.4.0",
    "prettier": "1.16.4",
    "prop-types": "15.7.1",
    "react": "16.8.1",
    "react-chartist": "0.13.3",
    "react-dom": "16.8.1",
    "react-google-maps": "9.4.5",
    "react-loader-spinner": "^2.3.0",
    "react-print-components": "^1.0.4",
    "react-redux": "^7.0.3",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.5",
    "react-select": "^2.4.3",
    "react-select-material-ui": "^2.1.1",
    "react-swipeable-views": "0.13.1",
    "react-to-pdf": "0.0.8",
    "react-to-print": "^2.1.2",
    "react-toastify": "^5.1.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "simple-react-validator": "^1.0.7",
    "update-electron-app": "^1.2.0"
  },
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "react-start": "react-scripts start",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "build-package-css": "cp src/assets/css/material-dashboard-react.css dist/material-dashboard-react.css",
    "build-package": "npm run build-package-css && babel src --out-dir dist",
    "electron-build": "electron-builder",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build ",
    "build-electron": " yarn electron-build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativetimofficial/material-dashboard-react.git"
  },
  "keywords": [],
  "author": "Creative Tim <hello@creative-tim.com> (https://www.creative-tim.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/creativetimofficial/material-dashboard-react/issues"
  },
  "homepage": "https://creativetimofficial.github.io/material-dashboard-react/#/dashboard",
  "optionalDependencies": {
    "@types/googlemaps": "3.30.16",
    "@types/markerclustererplus": "2.1.33",
    "ajv": "6.9.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-prettier": "3.0.1",
    "typescript": "^3.4.5",
    "wait-on": "^3.2.0"
  },
  "build": {
    "appId": "org.develar.onshape",
    "productName": "ITech",
    "icon": "build/icons",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.graphics-design"
    },
    "nsis": {
      "createDesktopShortcut": "always",
      "allowToChangeInstallationDirectory": true
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "category": "Graphics"
    }
  },
  "build-win": {
    "appId": "com.example.electron-cra",
    "productName": "ITech",
    "icon": "build/icons",
    "target": [
      "nsis-web"
    ],
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "build-linux": {
    "appId": "com.example.electron-cra",
    "productName": "ITech",
    "icon": "build/icons",
    "linux": {
      "icon": "build/icons",
      "category": "office",
      "target": [
        "AppImage",
        "deb"
      ],
      "maintainer": "...",
      "desktop": {
        "Encoding": "UTF-8"
      },
      "description": "An inventory systom for bearing, oil and Greece"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
